    "@langchain/groq": "^0.1.2",
    "@langchain/langgraph": "^0.2.36",
    "@langchain/openai": "^0.3.16",
    "@mysten/sui": "^1.19.0",
    "ai": "^4.0.22",
    "axios": "^1.7.9",
    "bignumber.js": "^9.1.2",
    "bn.js": "^5.2.1",
    "dotenv": "^16.4.7",
    "langchain": "^0.3.8",
